<config>
    <executableName>${app.executable}</executableName>
    <mainClass>${app.mainclass}</mainClass>
    <os>ios</os>    
    <target>ios</target>
    <iosInfoPList>Info.plist.xml</iosInfoPList>
    <resources>
        <resource>
            <directory>resources</directory>
        </resource>
    </resources>
    <forceLinkClasses>
        <pattern>com.android.org.conscrypt.**</pattern>
        <pattern>com.android.org.bouncycastle.**</pattern>
        <pattern>org.apache.harmony.security.provider.cert.DRLCertFactory</pattern>
        <pattern>org.apache.harmony.security.provider.crypto.CryptoProvider</pattern>
        <pattern>org.apache.harmony.security.provider.crypto.CryptoProvider</pattern>
        <pattern>sigmastate.**</pattern>
        <pattern>sigma.**</pattern>
        <pattern>scalan.**</pattern>
        <pattern>SQLite.**</pattern>
    </forceLinkClasses>
    <!-- Remove comment to automatically skip compilation and linking in IDEA -->
    <!-- <smartSkipRebuild>true</smartSkipRebuild> -->
</config>